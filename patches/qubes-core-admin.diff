+fi 
+
+
+ 
+if [ $1 = "remove" ] && [ -x /usr/bin/systemctl ]; then 
+        # Package removal, not upgrade 
+        /usr/bin/systemctl --no-reload disable --now qubes-qmemman.service || : 
+fi 
+
+
+ 
+if [ $1 = "remove" ] && [ -x /usr/bin/systemctl ]; then 
+        # Package removal, not upgrade 
+        /usr/bin/systemctl --no-reload disable --now qubesd.service || : 
+fi 
+
+
+if [ "$1" = "remove" ] ; then
