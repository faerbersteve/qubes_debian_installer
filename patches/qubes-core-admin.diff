+#!/bin/bash
+
+ 
+
+
+ 
+if [ $1 = "upgrade" ] && [ -x /usr/bin/systemctl ]; then 
+        # Package upgrade, not uninstall 
+        /usr/bin/systemctl try-restart qubes-qmemman.service || : 
+fi 
+
+
+ 
+if [ $1 = "upgrade" ] && [ -x /usr/bin/systemctl ]; then 
+        # Package upgrade, not uninstall 
+        /usr/bin/systemctl try-restart qubesd.service || : 
+fi 
+
+
+if [ "$1" = "remove" ] ; then
+	# no more packages left
+    chgrp root /etc/xen
+    chmod 700 /etc/xen
+    groupdel qubes
+fi
